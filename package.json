{
  "author": "The Snips Tooling Team",
  "license": "MIT",
  "description": "Node framework to bootstrap and deploy web applications based on templates",
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "pluralize": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "6.22.0",
    "concurrently": "^3.1.0",
    "jest": "18.1.0",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">= 6.0"
  },
  "main": "lib/index.js",
  "name": "teleport.js",
  "scripts": {
    "compile": "babel --presets latest -d lib/ src/",
    "postinstall": "rm -rf lib && mkdir -p lib && npm run compile",
    "link": "source bin/link.sh",
    "test": "export TEST_RANDOM=$RANDOM && jest tests/create.test.js && jest tests/methods.test.js",
    "watch": "nodemon lib/index.js --watch src --exec \"npm run compile\""
  },
  "bin": {
    "tpt": "bin/index.js"
  },
  "version": "0.1.1"
}
