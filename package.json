  "description": "Node framework to bootstrap and deploy applications based on templates",
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "json-stable-stringify": "^1.0.1",
    "mkdirp": "^0.5.1",
    "pluralize": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "jest": "18.1.0",
  },
  "main": "lib/index.js",
  "name": "teleport.js",
  "publishConfig": {
    "registry": "https://npm-registry.corp.snips.net"
  },
  "scripts": {
    "compile": "./node_modules/.bin/babel --presets es2015,stage-3 -d lib/ src/",
    "postinstall": "rm -rf lib && mkdir -p lib && npm run compile",
    "link": "source bin/link.sh",
    "watch": "./node_modules/nodemon/bin/nodemon.js src/index.js --watch src --exec babel-node --presets es2015,stage-3"
  },
  "bin": {
    "tpt": "bin/index.js"
  },
  "version": "0.1.1"
